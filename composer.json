{
  "name": "aigletter/yii2-logging",
  "type": "yii2-extension",
  "description": "Logging",
  "keywords": ["yii2", "logging"],
  "license": "MIT",
  "authors": [
    {
      "name": "Yurii Orlyk",
      "email": "aigletter@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4.0",
    "yiisoft/yii2": "~2.0.45",
    "kassner/log-parser": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0"
  },
  "autoload": {
    "psr-4": {
      "aigletter\\logging\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
